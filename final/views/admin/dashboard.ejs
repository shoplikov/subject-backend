<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Dashboard</title>
</head>
<body>
    <h1>Welcome to Admin Dashboard</h1>
    <ul>
        <% articles.forEach(article => { %>
            <li>
                <%= article.title %> - 
                <a href="/admin/edit/<%= article._id %>">Edit</a> | 
                <a href="/admin/delete/<%= article._id %>">Delete</a>
            </li>
        <% }); %>
    </ul>
</body>
</html>
